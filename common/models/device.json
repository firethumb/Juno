{
  "name": "device",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "forceId": false,
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "contractID": {
      "type": "string",
      "required": true
    },
    "licNo": {
      "type": "string",
      "required": true
    },
    "modelNo": {
      "type": "string",
      "required": true
    },
    "dateCreated": {
      "type": "date"
    },
    "dateLMod": {
      "type": "date"
    },
    "entryID": {
      "type": "string",
      "required": true
    },
    "serial": {
      "type": "string"
    },
    "wanIP": {
      "type": "string"
    },
    "config": {
      "type": "object"
    },
    "groupID": {
      "type": "string"
    },
    "active": {
      "type": "boolean"
    },
    "licEXPDTE": {
      "type": "date",
      "required": true
    },
    "lastHB": {
      "type": [
        "any"
      ]
    },
    "comments": {
      "type": "string"
    },
    "lastconfigDTE": {
      "type": "date"
    },
    "geoinfo": {
      "type": [
        "any"
      ]
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "ownerId"
    },
    "dashinfos": {
      "type": "hasOne",
      "model": "dashinfo",
      "foreignKey": "devID"
    },
    "logs": {
      "type": "hasOne",
      "model": "log",
      "foreignKey": "devID"
    },
    "settings": {
      "type": "hasOne",
      "model": "setting",
      "foreignKey": "devID"
    },
    "vouchers": {
      "type": "hasOne",
      "model": "voucher",
      "foreignKey": "devID"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "partner",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
